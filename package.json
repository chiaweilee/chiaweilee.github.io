{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "test": "umi test",
    "lint": "npm run lint:es && npm run lint:ts",
    "lint:es": "eslint --ext .js src mock tests",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "dist": "rundist",
    "server": "rundist --port 443 --https --sslkey cert/1604818_dear.red.key --sslcert cert/1604818_dear.red.pem --extend scripts/git-guard.js",
    "server:install": "npm install compression express github-webhook-handler node-cmd rundist",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "antd": "^3.12.1",
    "current-device": "^0.8.0",
    "dva": "^2.5.0-beta.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-react-jsx-filter": "^6.0.1",
    "banner-js-webpack-plugin": "^0.0.0-beta.0",
    "eslint": "^5.4.0",
    "eslint-config-ali": "^5.0.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-jsx-filter": "^6.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.7.0",
    "stylelint": "^9.10.1",
    "stylelint-config-ali": "^0.1.1",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.4.2",
    "umi-plugin-react": "^1.4.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:es",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "git add"
    ]
  }
}
