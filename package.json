{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "inspect": "vue inspect",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:report": "npm run build -- --report",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "dist": "rundist --port 8080 --extend scripts/git-guard.js",
    "server": "npm run dist",
    "server:install": "npm install compression express github-webhook-handler node-cmd",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "app.vue": "^0.1.10",
    "fastclick": "^1.0.6",
    "gsap": "^2.0.2",
    "pathfinding": "^0.4.18",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-progressbar-v": "^0.7.5",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-e2e-cypress": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.0.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "eslint-plugin-flowtype": "^3.2.0",
    "github-webhook-handler": "^0.7.1",
    "lint-staged": "^7.2.2",
    "node-cmd": "^3.0.0",
    "node-sass": "^4.9.0",
    "remove-flow-types-loader": "^1.1.0",
    "rundist": "^0.1.5",
    "sass-loader": "^7.0.1",
    "sass-variables-webpack-plugin": "^0.0.4",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ],
    "*.vue": [
      "npm run lint",
      "git add"
    ]
  }
}
